#define base0A #60837c
#define base0B #df8746
#define base0C #937b73
#define base0D #957064
#define base0E #dc4b4c
#define base0F #aa6845

*foreground:   base05
#ifdef background_opacity
*background:   [background_opacity]base00
#else
