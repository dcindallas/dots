! Start flavours
! Base16 Bright Circles
! Scheme: @duanecummins

#define base00 #25252f
#define base01 #454348
#define base02 #656160
#define base03 #847f79
#define base04 #a49d92
#define base05 #eeeeee
#define base06 #ccc5b7
#define base07 #c3bbaa
